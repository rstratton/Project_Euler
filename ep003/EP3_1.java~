import java.util.*;
import java.lang.*;

public class EP3_1{
	ArrayList<Long> factors = new ArrayList<Long>();
	ArrayList<Long> primes = new ArrayList<Long>();
	static Long testNum = 600851475143l;
	
	void generatePrimes(){
		for(long i = 2; i <= testNum; i++){
			System.out.println("Trying: " + i);
			for(long prime : primes){
				if(i % prime == 0)
					continue;
				else
					primes.add(i);
			}
		}
	}
	
	void generatePrimesTester(){
		generatePrimes();
		for(long prime : primes)
			System.out.println(prime);
	}
	
	void factorizeTester(){
		factorize();
		for(long factor: factors)
			System.out.println(factor);
	}
	
	void factorize(){
		long num = testNum;
		while(num > 1){
			for(long prime : primes){
				if(num % prime == 0){
					factors.add(prime);
					num /= prime;
				}
				else
					primes.remove(prime);
			}
		}
	}
	
	public static void main(String[] args){
		EP3_1 a = new EP3_1();
		a.generatePrimesTester();
		a.factorizeTester();
	}
}
			
	

